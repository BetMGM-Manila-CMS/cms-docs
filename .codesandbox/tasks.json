{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "docusaurus": {
      "name": "docusaurus",
      "command": "npm run docusaurus"
    },
    "start": {
      "name": "start",
      "command": "npm run start",
      "runAtStart": true,
      "preview": {
        "port": 3000
      }
    },
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "swizzle": {
      "name": "swizzle",
      "command": "npm run swizzle"
    },
    "deploy": {
      "name": "deploy",
      "command": "npm run deploy"
    },
    "clear": {
      "name": "clear",
      "command": "npm run clear"
    },
    "serve": {
      "name": "serve",
      "command": "npm run serve"
    },
    "write-translations": {
      "name": "write-translations",
      "command": "npm run write-translations"
    },
    "write-heading-ids": {
      "name": "write-heading-ids",
      "command": "npm run write-heading-ids"
    }
  }
}
